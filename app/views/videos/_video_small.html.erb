<div class='video'>
  <div class='video_embed'>
    <%= div_for(video, :width => 280, :height => 231, :class => 'small-video', :link => video.youtube_id) do %>
  <% end %>
  </div>

  <div class="video_title">
    <%= link_to video.name, video %>

    <div style='width:50px;float:right;padding-top:5px;overflow:hidden;margin-right:15px;'>
    <%= facebook_like(video).html_safe %>
  </div>
  </div>
</div>
