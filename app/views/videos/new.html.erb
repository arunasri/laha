<h1>New video</h1>

<%= form_for(@video, :class => "edit-video") do |f| %>
  <% if @video.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@video.errors.count, "error") %> 
        prohibited this feed from being saved:
      </h2>
      <ul>
        <% @video.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <table class="form_demo">
    <tbody>
      <tr>
        <th><%= f.label :url %></th>
        <td><%= f.text_field :url %></td>
      </tr>
      <tr>
        <th>
        </th>
        <td><%= f.submit %></td>
      </tr>
    </tbody>
  </table>
<% end %>

<%= link_to 'Back', admin_path %>
